    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/selectedColor</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryVariant</item>
        <item name="android:colorPrimaryDark">@color/colorPrimaryVariant</item>
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="colorSurface">@color/colorSurface</item>
        <item name="android:colorBackground">#FFFFFF</item>
        <item name="colorError">@color/colorError</item>
        <item name="colorOnPrimary">@color/colorOnPrimary</item>
        <item name="colorOnSecondary">@color/colorOnSecondary</item>
        <item name="colorOnSurface">@color/colorOnSurface</item>
        <item name="colorOnError">@color/colorOnError</item>
        <item name="colorOnBackground">@color/colorOnSurface</item>
        <item name="colorSecondaryVariant">@color/colorSecondaryVariant</item>
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:actionMenuTextColor">@color/colorSurface</item>
        <item name="android:actionMenuTextAppearance">@style/MenuText</item>
        <item name="materialAlertDialogTheme">@style/MyThemeOverlay.MaterialComponents.MaterialAlertDialog</item>
        <item name="android:windowLightStatusBar" tools:targetApi="M">true</item>
        <!--<item name="android:timePickerStyle">@style/MyTimePickerDialogStyle</item>
        <item name="android:datePickerStyle">@style/MyDatePickerDialogStyle</item>
        <item name="android:buttonBarNegativeButtonStyle">@style/TimePicker.ButtonBarNegativeButtonStyle</item>
        <item name="android:buttonBarPositiveButtonStyle">@style/TimePicker.ButtonBarNegativeButtonStyle</item>-->

    </style>


    <style name="ToobarTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:statusBarColor">@color/splesh_dark_color</item>
        <item name="android:windowDisablePreview">true</item>
    </style>
    <style name="loginTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowLightStatusBar">false</item>
