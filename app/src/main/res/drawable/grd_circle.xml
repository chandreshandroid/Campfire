<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="ring">
            <gradient
                android:angle="270"
                android:startColor="#f05e23"
                android:endColor="#f05e23"/>
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="90"
                android:endColor="#feb902"
                android:startColor="#f05e23"/>
        </shape>
    </item>
</layer-list>